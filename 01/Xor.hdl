/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=nota);
    Not (in=b, out=notb);
    Nand (a=nota, b=b, out=o1);
    Nand (a=a, b=notb, out=o2);
    Nand (a=o1, b=o2, out=out);
}
